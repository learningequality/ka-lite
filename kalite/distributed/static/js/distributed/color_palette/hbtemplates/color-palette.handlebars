.k-bg-clr {
  background-color: {{k_bg_clr}};
}
.k-bg {
  color: {{k_bg_clr}};
}

.k-acn-clr {
  background-color: {{k_acn_clr}} !important;
}
.k-acn {
  color: {{k_acn_clr}} !important;
}
.k-acn-hover {
  background-color: {{k_acn_clr}};
}
.k-acn-hover:hover, .k-acn-hover:focus {
  color: {{k_acn_clr}};
}
.k-acn-border {
  border-color: {{k_acn_clr}} !important;
}
.k-acn-border-hover:hover {
  border-color: {{k_acn_clr}};
}


.k-act-clr {
  background-color: {{k_act_clr}};
}
.k-act {
  color: {{k_act_clr}};
}


.k-hln-clr {
  background-color: {{k_hln_clr}};
}
.k-hln {
  color: {{k_hln_clr}};
}


.k-byt-clr {
  background-color: {{k_byt_clr}};
}
.k-byt {
  color: {{k_byt_clr}};
}


/*
corner cases
*/
.nav-tabs li.active a{
  color: {{k_acn_clr}};
}
.nav-tabs li a{
  color: white;
}

.active-nav {
  color: {{k_acn_clr}};
}

.navbar-collapse .ui-state-hover,
.navbar-collapse .ui-widget-content .ui-state-hover,
.navbar-collapse .ui-widget-header .ui-state-hover,
.navbar-collapse .ui-state-focus,
.navbar-collapse .ui-widget-content .ui-state-focus,
.navbar-collapse .ui-widget-header .ui-state-focus {
  background: {{k_acn_clr}};
}

li span.fancytree-selected {
  border-color: {{k_act_clr}};
  background-color: {{k_act_clr}};
}
span.fancytree-expanded {
  border-color: {{k_acn_clr}};
  background-color: {{k_acn_clr}};
}

.btn-success {
    background-color: {{k_acn_clr}} !important;
    background-image: none !important;
}